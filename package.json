    "immer": "^7.0.9"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "dotenv-load": "^2.0.0",
    "stylus": "^0.54.8",
    "typescript": "^3.9.3",
    "vite": "^1.0.0-rc.1",
    "vite-plugin-react": "^3.0.0"
  }
